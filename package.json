{
  "name": "TpCAnalyzer",
	"displayName": "tp-c-cpp-analyzer",
	"description": "C Cpp Analyzer",
	"publisher": "toanpi",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.69.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:TpCAnalyzer.heh"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "TpCAnalyzer.heh",
				"title": "He he"
			}
		]
	},
	"repository": {
    "type": "git",
    "url": "https://github.com/toanpi/tp-static-analysis-c-c-.git"
  },
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"install": "vsce package && code --install-extension TpCAnalyzer-0.0.1.vsix",
		"package": "vsce package",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@types/vscode": "^1.69.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.18.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4",
		"@vscode/test-electron": "^2.1.5"
	}
}
